{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Front End",
  "homepage": "https://github.com/maxiaochuan/frontend#readme",
  "bugs": {
    "url": "https://github.com/maxiaochuan/frontend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxiaochuan/frontend.git"
  },
  "author": "Xiaochuan Ma <mxcins@gmail.com>",
  "scripts": {
    "deploy": "npx pm2 deploy production update",
    "start": "umi dev",
    "build": "umi build",
    "lint": "yarn lint:style && yarn lint:ts && yarn ts-compile-check",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint-staged:ts": "tslint --fix \"src/**/*.ts*\"",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:ts": "tslint --fix \"src/**/*.ts*\"",
    "prettier": "prettier --write \"./src/**/*\" \"./typings/**/*\"",
    "test": "umi test",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ts-compile-check && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": "npm run lint-staged:ts"
  },
  "dependencies": {
    "@mxcins/lodash": "1.0.0",
    "@mxcins/pluralize": "0.1.1",
    "@mxcins/request": "1.4.3",
    "@mxcins/utils": "1.5.1",
    "antd": "3.21.2",
    "lodash-es": "4.17.15",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "socket.io-client": "2.2.0",
    "umi": "2.8.18",
    "umi-plugin-react": "1.9.17"
  },
  "devDependencies": {
    "@mxcins/types": "1.3.1",
    "@types/debug": "^4.1.4",
    "@types/lodash-es": "^4.17.3",
    "@types/socket.io-client": "^1.4.32",
    "debug": "^4.1.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "ssh2": "^0.8.5",
    "stylelint": "9.x",
    "stylelint-config-mxcins": "1.0.0",
    "tslint": "5.16.0",
    "tslint-config-mxcins": "1.0.0",
    "typescript": "3.5.2",
    "umi-types": "0.3.15"
  },
  "resolutions": {
    "@types/react": "16.8.14"
  }
}
