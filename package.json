{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "autod": "autod",
    "build": "cross-env UMI_ENV=prod APP_ROOT=app/web HTML=none umi build",
    "ci": "npm run lint && npm run cov",
    "cov": "egg-bin cov",
    "debug": "egg-bin debug",
    "dev": "egg-bin dev",
    "lint": "eslint . --fix && yarn ts-compile-check",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"{,!(node_modules|build)/**/}*.{js,ts,jsx,tsx,less,md}\"",
    "start": "yarn build && egg-scripts start --daemon --title=egg-server-frontend  --sticky",
    "start:debug": "yarn build && egg-scripts start --title=egg-server-frontend  --sticky",
    "stop": "egg-scripts stop --title=egg-server-frontend",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit"
  },
  "resolutions": {
    "@types/react": "16.9.8",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "5.1.2",
    "@types/react-router-dom": "5.1.2",
    "antd": "3.25.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ts-compile-check && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{less}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@mxcins/extensions": "^1.0.1",
    "@mxcins/request": "2.1.0-alpha.0",
    "@mxcins/utils": "^1.7.1",
    "antd": "3.25.2",
    "egg": "^2.15.1",
    "egg-proxy": "^1.1.0",
    "egg-scripts": "^2.11.0",
    "egg-socket.io": "^4.1.6",
    "egg-view-assets": "^1.6.0",
    "husky": "4.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "9.5.0",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "moment": "^2.24.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "socket.io-client": "^2.3.0",
    "typescript": "3.6.2",
    "umi": "^2.12.9",
    "umi-plugin-locale": "^2.11.4",
    "umi-plugin-react": "^1.14.12",
    "umi-server": "^1.2.1"
  },
  "devDependencies": {
    "@mxcins/bedrock": "^1.0.0-beta.4",
    "@mxcins/types": "^1.5.0",
    "@types/js-cookie": "^2.2.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/react": "16.9.8",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "5.1.2",
    "@types/react-router-dom": "5.1.2",
    "@types/socket.io-client": "^1.4.32",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^6.0.3",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ci": {
    "version": "10"
  },
  "egg": {
    "declarations": true
  }
}
